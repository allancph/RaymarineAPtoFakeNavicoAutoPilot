{
  "name": "@canboat/canboatjs",
  "version": "2.11.0",
  "description": "Native javascript version of canboat",
  "main": "index.js",
  "type": "module",
  "exports": "./index.js",
  "scripts": {
    "build": "tsc",
    "dev-test": "jest --watch",
    "test": "jest && mocha --exit",
    "create-release": "github-create-release --owner canboat --repository canboatjs",
    "release": "npm run build && git tag -d v$npm_package_version && git tag v$npm_package_version && git push --tags && git push && npm run create-release"
  },
  "bin": {
    "analyzerjs": "./bin/analyzerjs",
    "to-pgn": "./bin/to-pgn",
    "candumpanalyzerjs": "./bin/candumpanalyzerjs",
    "actisense-serialjs": "./bin/actisense-serialjs"
  },
  "jest": {
    "rootDir": "lib"
  },
  "keywords": [
    "boat",
    "bus",
    "can",
    "canboat",
    "k",
    "marine",
    "nmea2000",
    "parser",
    "pgn",
    "signalk",
    "signal"
  ],
  "author": "Scott Bender <scott@scottbender.net>",
  "contributors": [
    {
      "name": "Kees Verruijt",
      "email": "kees@verruijt.net"
    },
    {
      "name": "Teppo Kurki",
      "email": "teppo.kurki@iki.fi"
    },
    {
      "name": "Jouni Hartikainen",
      "email": "jouni.hartikainen@iki.fi"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@canboat/pgns": "^2.0.0",
    "bit-buffer": "latest",
    "date-fns": "^2.30.0",
    "debug": "^4.3.4",
    "dnssd": "^0.5.0",
    "int64-buffer": "^0.1.11",
    "lodash": "^4.17.21",
    "mqtt": "^4.3.7",
    "split": "^1.0.1"
  },
  "devDependencies": {
    "@signalk/github-create-release": "^1.1.0",
    "chai": "^4.3.7",
    "chai-json-equal": "^1.0.0",
    "chai-string": "^1.9.0",
    "chai-things": "^0.2.1",
    "jest": "^29.6.0",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "typescript": "^5.0.0"
  },
  "optionalDependencies": {
    "serialport": "^12.0.0",
    "socketcan": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/canboat/canboatjs.git"
  }
